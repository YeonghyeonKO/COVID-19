filter(항목 == "인구밀도[㎢당]") %>%
mutate(new16 = case_when(!is.na(X2016.년) ~ X2016.년,
is.na(X2016.년) ~ X2015.년,
TRUE ~ NA_real_),
new17 = case_when(!is.na(X2017.년) ~ X2017.년,
is.na(X2017.년) ~ X2016.년,
TRUE ~ NA_real_),
density = case_when(!is.na(new17) ~ new17,
is.na(new17) ~ new16,
TRUE ~ NA_real_)) %>%
select(1, 10)
names(pop_density) <- c("Country", "pop_density")
